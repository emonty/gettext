#: src/format-awk.c:489 src/format-elisp.c:337 src/format-librep.c:301
#: src/format-pascal.c:390 src/format-perl.c:567 src/format-php.c:337
#: src/format-qt.c:132 src/format-tcl.c:376 src/format-ycp.c:133
#, c-format
