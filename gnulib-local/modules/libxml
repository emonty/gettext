Makefile.am:
BUILT_SOURCES += $(LIBXML_H)

libxml/DOCBparser.h: libxml/DOCBparser_.h
	test -d libxml || mkdir libxml
	{ echo '/* DO NOT EDIT! GENERATED AUTOMATICALLY! */'; \
	  cat $(srcdir)/libxml/DOCBparser_.h; \
	} > $@-t
	mv -f $@-t $@

